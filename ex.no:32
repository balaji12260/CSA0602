#include <stdio.h>

int main() {
    int i, n;

    // Example input (you can modify manually for test cases)
    // Test Case 1
    int a1[] = {5,7,3,4,9,12,6,2};
    int n1 = 8;

    // Test Case 2
    int a2[] = {1,3,5,7,9,11,13,15,17};
    int n2 = 9;

    // Test Case 3
    int a3[] = {22,34,35,36,43,67,12,13,15,17};
    int n3 = 10;

    // Function to compute min and max
    void findMinMax(int arr[], int n) {
        int min = arr[0], max = arr[0];

        for (int i = 1; i < n; i++) {
            if (arr[i] < min)
                min = arr[i];

            if (arr[i] > max)
                max = arr[i];
        }

        printf("Min = %d, Max = %d\n", min, max);
    }

    printf("Test Case 1:\n");
    findMinMax(a1, n1);

    printf("\nTest Case 2:\n");
    findMinMax(a2, n2);

    printf("\nTest Case 3:\n");
    findMinMax(a3, n3);

    return 0;
}
