#include <stdio.h>

void selectionSort(int arr[], int n) {
    int i, j, minIndex, temp;
    for (i = 0; i < n - 1; i++) {
        minIndex = i;
        for (j = i + 1; j < n; j++) {
            if (arr[j] < arr[minIndex])
                minIndex = j;
        }
        // Swap smallest with current position
        temp = arr[i];
        arr[i] = arr[minIndex];
        arr[minIndex] = temp;
    }
}

void printArray(int arr[], int n) {
    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);
    printf("\n");
}

int main() {
    int arr1[] = {5, 2, 9, 1, 5, 6};
    int arr2[] = {10, 8, 6, 4, 2};
    int arr3[] = {1, 2, 3, 4, 5};

    int n1 = sizeof(arr1) / sizeof(arr1[0]);
    int n2 = sizeof(arr2) / sizeof(arr2[0]);
    int n3 = sizeof(arr3) / sizeof(arr3[0]);

    printf("Original Random Array: ");
    printArray(arr1, n1);
    selectionSort(arr1, n1);
    printf("Sorted Random Array: ");
    printArray(arr1, n1);

    printf("\nOriginal Reverse Sorted Array: ");
    printArray(arr2, n2);
    selectionSort(arr2, n2);
    printf("Sorted Reverse Array: ");
    printArray(arr2, n2);

    printf("\nOriginal Already Sorted Array: ");
    printArray(arr3, n3);
    selectionSort(arr3, n3);
    printf("Sorted Already Sorted Array: ");
    printArray(arr3, n3);

    return 0;
}
